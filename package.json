{
  "name": "hackernews",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@9.4.0",
  "scripts": {
    "\n========= Setup ==========": "",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "\n========== Lint ==========": "",
    "lint": "pnpm run '/^lint:.*/'",
    "lint:eslint": "eslint --cache src",
    "lint:tsc": "tsc --noEmit",
    "\n========== Test ==========": "",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-query": "^3.39.3"
  },
  "devDependencies": {
    "@josephmcg/eslint-config-react": "^1.0.5",
    "@josephmcg/prettier-config": "^1.0.2",
    "@josephmcg/tsconfig": "^1.0.0",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^22.3.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "husky": "^9.1.4",
    "jsdom": "^24.1.1",
    "lint-staged": "^15.2.9",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vitest": "^2.0.5"
  }
}
