{
  "name": "hackernews",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "\n========= Setup ==========": "",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "\n========== Lint ==========": "",
    "lint": "pnpm run '/^lint:.*/'",
    "lint:eslint": "eslint --cache src",
    "lint:tsc": "tsc --noEmit",
    "\n========== Test ==========": "",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-query": "^3.39.3"
  },
  "devDependencies": {
    "@josephmcg/eslint-config-react": "^0.0.38",
    "@josephmcg/prettier-config": "^0.0.15",
    "@josephmcg/tsconfig": "^0.0.9",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "clsx": "^2.1.1",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  }
}
